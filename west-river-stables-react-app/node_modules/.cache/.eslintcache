Route exact path=\"/services\" component={Services} />\n          <Route exact path=\"/contact\" component={Contact} />\n          <Route exact path=\"/login\" component={LoginContainer} />\n          <Route component={NotFound} />\n        </Switch>\n        <Footer />\n      </div>\n    );\n  }\n}  \n\nconst mapStateToProps = state => {\n  return ({\n    loggedIn: !!state.currentUser\n    // currentUser: state.currentUser,\n    // horses: state.myHorses\n  })\n}\n\nexport default connect(mapStateToProps, { getCurrentUser })(App);\n","/Users/jaimerogstad/Flatiron/code/west-river-stables-website/west-river-stables-react-app/src/reportWebVitals.js",[],"/Users/jaimerogstad/Flatiron/code/west-river-stables-website/west-river-stables-react-app/src/components/About.js",[],"/Users/jaimerogstad/Flatiron/code/west-river-stables-website/west-river-stables-react-app/src/components/Contact.js",[],"/Users/jaimerogstad/Flatiron/code/west-river-stables-website/west-river-stables-react-app/src/components/Footer.js",[],"/Users/jaimerogstad/Flatiron/code/west-river-stables-website/west-river-stables-react-app/src/components/NavBar.js",[],"/Users/jaimerogstad/Flatiron/code/west-river-stables-website/west-river-stables-react-app/src/components/NotFound.js",[],"/Users/jaimerogstad/Flatiron/code/west-river-stables-website/west-river-stables-react-app/src/components/Services.js",[],"/Users/jaimerogstad/Flatiron/code/west-river-stables-website/west-river-stables-react-app/src/components/Login.js",[],"/Users/jaimerogstad/Flatiron/code/west-river-stables-website/west-river-stables-react-app/src/containers/HomeContainer.js",[],"/Users/jaimerogstad/Flatiron/code/west-river-stables-website/west-river-stables-react-app/src/components/Bios.js",[],"/Users/jaimerogstad/Flatiron/code/west-river-stables-website/west-river-stables-react-app/src/components/Button.js",[],"/Users/jaimerogstad/Flatiron/code/west-river-stables-website/west-river-stables-react-app/src/components/HeroSection.js",[],"/Users/